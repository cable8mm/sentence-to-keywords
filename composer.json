{
    "name": "cable8mm/sentence-to-keywords",
    "description": "Use a search engine(naver.com) to extract important keywords from sentences and correct typos.",
    "keywords": [
        "sentence",
        "keywords",
        "searchengine"
    ],
    "type": "project",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "laravel/pint": "^1.10"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Cable8mm\\SentenceToWords\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cable8mm\\SentenceToWords\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "./vendor/bin/phpunit tests"
        ],
        "lint": "./vendor/bin/pint"
    }
}